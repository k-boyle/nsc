native import std.stdio;

Int main() {
    (10+20)!;
    Int a;
    a := (10+20);
    a!;
    0;
};

Int infix_+_(Int a, Int b) {
    add(a,b);
};

Nothing postfix_!_(Int a) {
    writeln(a);
};
Nothing infix_:=_(Int* a, Int b) {
    assignInt(a,b);
};

native Nothing writeln(Int a);
native Int add(Int a, Int b);
native Nothing assignInt(Int* a, Int b);
